<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.app.tiffinapp">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:usesCleartextTraffic="true">

        <activity android:name=".SplashActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".DashboardActivity"
            android:screenOrientation="portrait"></activity>
        <activity
            android:name=".LoginActivity"
            android:screenOrientation="portrait"></activity>
        <activity
            android:name=".RegisterActivity"
            android:screenOrientation="portrait"></activity>
        <activity
            android:name=".OperatorActivity"
            android:screenOrientation="portrait"></activity>
        <activity
            android:name=".UserActivity"
            android:screenOrientation="portrait"></activity>
        <activity
            android:name=".AllotedDeliveryList"
            android:screenOrientation="portrait"></activity>
        <activity
            android:name=".AllotedActivity"
            android:screenOrientation="portrait"></activity>
        <activity
            android:name=".DeliveryBoys"
            android:screenOrientation="portrait"></activity>
        <activity
            android:name=".DeliveryStatus"
            android:screenOrientation="portrait"></activity>
        <activity
            android:name=".SubscriptionZone"
            android:screenOrientation="portrait"></activity>
        <activity
            android:name=".BuyOnceZone"
            android:screenOrientation="portrait"></activity>
        <activity
            android:name=".MyProfile"
            android:screenOrientation="portrait"></activity>
        <activity
            android:name=".AddAddress"
            android:screenOrientation="portrait"></activity>
        <activity
            android:name=".AddressList"
            android:screenOrientation="portrait"></activity>
        <activity
            android:name=".SubscriptionOrder"
            android:screenOrientation="portrait"></activity>
        <activity
            android:name=".ConfirmDeliveryAddress"
            android:screenOrientation="portrait"></activity>
        <activity
            android:name=".PaymentOptionActivity"
            android:screenOrientation="portrait"></activity>
        <activity
            android:name=".OrderConfirmationActivity"
            android:screenOrientation="portrait"></activity>


    </application>
</manifest>